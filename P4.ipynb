    "\n",
    "\n",
    "def centimeter(n):\n",
    "    print(f\"{n} centimeter is : {n*(1/2.54)}inches\")\n",
    "\n",
    "def meter(n):\n",
    "    print(f\"{n} meter is : {n*39.37}inches\")\n",
